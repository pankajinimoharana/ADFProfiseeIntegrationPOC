{
	"name": "CopyCSVFileToProfiseeStaging",
	"properties": {
		"activities": [
			{
				"name": "Copy B2C Customer File To Staging",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "profisee-input/b2c_customer/B2CCustomers.csv"
					},
					{
						"name": "Destination",
						"value": "[stg].[tB2C_Customer_Merge].Azure Data Lake Storage Gen2 file"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": false,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "SourceId",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Code"
								}
							},
							{
								"source": {
									"name": "CustomerName",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Name"
								}
							},
							{
								"source": {
									"name": "Phone",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Phone Number"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_B2C",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ProfiseePaas01",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}